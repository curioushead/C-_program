#include<iostream>
#include<string.h>
using namespace std;
class student{
char name[20];
int rollno;
public:
    student()
    {
       // name="Ravi";
    strcpy(name,"Ravi");
        rollno=1;
    }
    void get()
    {
        cout<<"Enter your name and roll no"<<endl;
        cin>>name>>rollno;
    }
    void display()
    {

        cout<<"Name: "<<name<<endl<<"Roll no: "<<rollno<<endl;
    }
};
int main()
{
    student s;
    s.display();
    s.get();
    s.display();
}

